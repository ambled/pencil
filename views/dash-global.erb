<% clusters =  @dash.clusters() %>
<% links = suggest_cluster_links(clusters) %>
<%= css_url %>

<%= @dash.graphs.collect { |g| "<a href=\"##{g.name}\">#{g.name}</a>" }.join(" ") %>

<%= cluster_switcher %>

<% @dash.graphs.each do |g| %>
  <div class="graphsection">
    <a name="<%= g.name %>">
    <span class="graphtitle"><%= g.name %></span>
    <span class="dashlinks"><%= links %></span>
    <div class="graph">
  <img src="<%= @dash.render_global_graph(g, :dynamic_url_opts => merge_opts) %>" >
    </div>
  </div>
<% end %>
